  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\CefSharp.AppHost.Example.Remote.Services\CefSharp.AppHost.Example.Remote.Services.csproj" />
    <ProjectReference Include="..\CefSharp.AppHost.Interfaces\CefSharp.AppHost.Interfaces.csproj" />
    <ProjectReference Include="..\CefSharp.AppHost.Server\CefSharp.AppHost.Server.csproj" />
  </ItemGroup>
</Project>